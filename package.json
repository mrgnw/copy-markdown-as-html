{
    "name": "copy-markdown-as-html",
    "displayName": "Copy Markdown as HTML",
    "description": "Copies the selected markdown text to the clipboard as HTML",
    "repository": {
        "type": "git",
        "url": "https://github.com/jerriep/copy-markdown-as-html"
    },
    "version": "1.0.0",
    "publisher": "jerriepelser",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyAsHtml"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyAsHtml",
                "title": "Markdown: Copy as HTML"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "clipboardy": "^1.2.3",
        "markdown-it": "^8.4.1"
    }
}
